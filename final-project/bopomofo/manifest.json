{
	"name": "Bopomofo Keyboard",
	"version": "0.01",
	"description": "Type and copy Bopomofo (Zhuyin) characters",
	"permissions": ["activeTab"],
	"browser_action": {
		"default_popup": "popup.html",
		"default_icon": {
			"16": "images/icon16.png",
			"32": "images/icon32.png",
			"128": "images/icon128.png"
		}
	},
	"icons": {
		"16": "images/icon16.png",
		"32": "images/icon32.png",
		"128": "images/icon128.png"
	},
	"manifest_version": 2
}